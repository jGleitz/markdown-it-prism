{
  "name": "markdown-it-prism",
  "version": "2.0.5",
  "description": "Highlights code blocks in markdown-it using Prism.",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "highlight",
    "prism",
    "prismjs"
  ],
  "homepage": "https://github.com/jGleitz/markdown-it-prism",
  "license": "GPL-3.0",
  "author": {
    "name": "Joshua Gleitze",
    "url": "https://joshuagleitze.de"
  },
  "files": [
    "README.md",
    "LICENSE",
    "build/index.js",
    "CHANGELOG.md"
  ],
  "main": "build/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jGleitz/markdown-it-prism.git"
  },
  "scripts": {
    "build": "babel *.js -d build",
    "lint": "eslint *.js",
    "prepare": "npm run build",
    "unittest": "mocha test.js",
    "test": "npm run lint && npm run unittest"
  },
  "dependencies": {
    "prismjs": "1.20.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/register": "7.10.3",
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "chai": "4.2.0",
    "chai-string": "1.5.0",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "markdown-it": "11.0.0",
    "mocha": "8.0.1",
    "semantic-release": "17.1.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "mocha": {
    "require": "@babel/register"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# [Changelog](http://keepachangelog.com/)\n\nThis project adheres to [Semantic Versioning](http://semver.org/)."
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
