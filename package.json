{
  "name": "markdown-it-prism",
  "version": "2.1.1",
  "description": "Highlights code blocks in markdown-it using Prism.",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "highlight",
    "prism",
    "prismjs"
  ],
  "homepage": "https://github.com/jGleitz/markdown-it-prism",
  "license": "GPL-3.0",
  "author": {
    "name": "Joshua Gleitze",
    "url": "https://joshuagleitze.de"
  },
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "build/index.js",
    "build/index.d.ts"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jGleitz/markdown-it-prism.git"
  },
  "scripts": {
    "build:transpile": "babel ./src --out-dir build --extensions '.ts'",
    "build:types": "tsc --project src",
    "lint": "npm-run-all lint:*",
    "lint:types": "tsc",
    "lint:style": "eslint .",
    "lint:commits": "commitlint --from=semantic-commit-start --to=HEAD",
    "prepack": "npm-run-all build:*",
    "unittest": "jest",
    "test": "npm-run-all lint:* unittest"
  },
  "dependencies": {
    "prismjs": "1.21.0"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@babel/register": "7.11.5",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.13",
    "@types/markdown-it": "10.0.2",
    "@types/prismjs": "1.16.1",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "eslint": "7.8.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "markdown-it": "11.0.0",
    "npm-run-all": "4.1.5",
    "semantic-release": "17.1.1",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# [Changelog](http://keepachangelog.com/)\n\nThis project adheres to [Semantic Versioning](http://semver.org/)."
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
